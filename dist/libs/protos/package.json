{
  "name": "@straufi/protos",
  "version": "0.0.1",
  "scripts": {
    "proto-lint": "protolint -config_path=../../protolint.yaml .",
    "proto-fix": "protolint -fix -config_path=../../protolint.yaml .",
    "gen": "concurrently \"npm run gen:base\" \"npm run gen:nestjs\"",
    "gen:base": "protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/generated/base --ts_proto_opt=lowerCaseServiceMethods=true --ts_proto_opt=outputServices=grpc-js --ts_proto_opt=esModuleInterop=true --ts_proto_opt=outputIndex=true --ts_proto_opt=exportCommonSymbols=false --ts_proto_opt=useDate=true ./src/definitions/**/*.proto",
    "gen:nestjs": "protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/generated/nestjs --ts_proto_opt=nestJs=true --ts_proto_opt=esModuleInterop=true --ts_proto_opt=outputIndex=true --ts_proto_opt=exportCommonSymbols=false --ts_proto_opt=useDate=true ./src/definitions/**/*.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.2",
    "@grpc/grpc-js": "^1.12.2",
    "@nestjs/microservices": "^10.4.7",
    "@swc/helpers": "~0.5.13",
    "protobufjs": "7.4.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "protoc": "^1.1.3",
    "protolint": "^0.50.5",
    "ts-proto": "^2.2.5"
  },
  "types": "./src/index.d.ts",
  "main": "./src/index.js",
  "type": "commonjs"
}